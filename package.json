{
  "name": "wct-tdd-runner",
  "version": "1.0.0",
  "description": "Run your web compontent tests in a TDD fashion, using chrome in headless mode.",
  "bin": {
    "ag": "./bin/wcttdd"
  },
  "keywords": [
    "browser",
    "polymer",
    "test",
    "testing",
    "web component",
    "web",
    "tdd"
  ],
  "homepage": "https://github.com/JeffLeFoll/wct-tdd-runner",
  "bugs": "https://github.com/JeffLeFoll/wct-tdd-runner/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeffLeFoll/wct-tdd-runner.git"
  },
  "main": "lib/plugin.js",
  "scripts": {
    "test": "mocha **/*_spec.js --reporter spec --recursive",
    "test:cover": "istanbul cover _mocha -- -u exports -R spec ./lib/**/*_spec.js",
    "test:coverAppveyor": "istanbul cover node_modules/mocha/bin/_mocha -- -u exports -R mocha-appveyor-reporter ./lib/**/*_spec.js",
    "lint": "eslint .; exit 0"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "chokidar": "^1.7.0",
    "chrome-launcher": "^0.3.1",
    "chrome-remote-interface": "^0.24.0",
    "fs-extra": "^4.0.1",
    "klaw": "^2.1.0",
    "nearest-color": "^0.4.0",
    "prpl-server": "^0.9.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "mocha": "^3.5.0"
  },
  "engines": {
    "node": ">= 6.0"
  }
}
